#include <bits/stdc++.h>

using namespace std;

int main()
{
    int X , Y;

	cin >> X >> Y;


	cout << X << " + "<< Y << " = "<< X+Y <<"\n";
	cout << X << " * "<< Y << " = "<< X*Y <<"\n";
	cout << X << " - "<< Y << " = "<< X-Y <<"\n";

	return 0;
}



