#include <bits/stdc++.h>

using namespace std;

int main()
{
	long long N;
	cin >> N;

	if(N >= 365)
	{
		cout << N/365 << " years"<<"\n";
		if( (N - 365) >= 30 )
		{
			cout << (N - 365)/30 << " months"<<"\n";
			cout << N-365 << " days"<<"\n";
		}
		else
		{
			cout << 0 << " months"<<"\n";
			cout << N-365 << " days"<<"\n";
		}
	}

	return 0;
}




