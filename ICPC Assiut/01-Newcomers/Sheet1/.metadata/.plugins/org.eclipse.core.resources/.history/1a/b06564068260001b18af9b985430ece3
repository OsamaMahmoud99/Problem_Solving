#include <bits/stdc++.h>

using namespace std;

int main()
{
	unsigned long  int A , B;
	char S;
	cin >> A >> S >> B;

	if(S == '+')
		cout << A+B;
	else if(S == '-')
		cout << A-B;
	else if(S == '*')
		cout << A*B;
	else if(S == '/')
		cout << A/B;

	return 0;
}




