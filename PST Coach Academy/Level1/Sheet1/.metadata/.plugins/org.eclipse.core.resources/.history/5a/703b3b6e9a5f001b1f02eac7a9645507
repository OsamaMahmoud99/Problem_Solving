#include <bits/stdc++.h>

using namespace std;

int main()
{
	int t , count =0;
	unsigned long long a , b;

	cin >> t;

	for(int i=0; i<t; i++)
		cin >> a >> b;

	for(int i=0; i<t; i++)
	{
		while(a%b !=0)
		{
			a++;
			count++;
		}

		a = a-count;
		cout << count <<"\n";
		count = 0;

	}


	return 0;
}




