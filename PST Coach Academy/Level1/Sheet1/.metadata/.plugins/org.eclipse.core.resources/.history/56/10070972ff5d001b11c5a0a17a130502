#include <bits/stdc++.h>

using namespace std;

int main()
{
	int x , step = 0 , min = 0;
	cin >> x;

	for(int i=1; i<=5; i++)
	{
		int res = x / i;

		if(res < min)
			min = res;
	}
	cout << min << "\n";




	return 0;
}




