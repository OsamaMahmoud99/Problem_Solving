#include <bits/stdc++.h>

using namespace std;

int main()
{
	int m , n ,a , flag1 = 0 , flag2 = 0;

	cin >> n >> m >> a;

	if(m % a)
		flag1 = 1;
	if(n % a)
		flag2 = 1;

	cout << (m/a+flag1) * (n/a+flag2)<<"\n";


	return 0;
}




