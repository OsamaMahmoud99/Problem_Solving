#include <bits/stdc++.h>

using namespace std;

int main()
{
	int x  , min = 5s;
	cin >> x;

	for(int i=1; i<=5; i++)
	{
		int res = x / i;

	   if( x % i == 0)
		if(res < min)
			min = res;
	}
	cout << min << "\n";
	return 0;
}




