#include <bits/stdc++.h>

using namespace std;

int main()
{
	int t;
	unsigned long long a , b;

	cin >> t;

	for(int i=1; i<=t; i++)
	{
		cin >> a >> b;

		if(a % b == 0)
			cout << 0 <<"\n";
		else
			cout << (b - (a%b)) <<"\n";

	}

	return 0;
}




